{
  "name": "find-my-repository",
  "version": "0.1.0",
  "homepage": "https://github.com/Lorellana21/find-my-repository",
  "private": true,
  "dependencies": {
    "@emotion/react": "^11.9.0",
    "@emotion/styled": "^11.8.1",
    "@mui/icons-material": "^5.8.2",
    "@mui/material": "^5.8.1",
    "@testing-library/jest-dom": "^5.16.4",
    "@testing-library/react": "^13.3.0",
    "@testing-library/user-event": "^13.5.0",
    "@types/node": "^16.11.36",
    "@types/react": "^18.0.9",
    "@types/react-dom": "^18.0.5",
    "axios": "^0.27.2",
    "react": "^18.1.0",
    "react-dom": "^18.1.0",
    "react-scripts": "5.0.1",
    "sass": "^1.52.1",
    "typescript": "^4.7.2",
    "web-vitals": "^2.1.4"
  },
  "scripts": {
    "prepare": "husky install",
    "start": "react-scripts start",
    "build": "react-scripts build",
    "test": "jest --coverage --passWithNoTests",
    "eject": "react-scripts eject",
    "lint": "semistandard --fix",
    "cy:open": "cypress open",
    "cy:test": "cypress run",
    "instrumented": "nyc instrument --compact=false src instrumented",
    "clean:cov": "rm -rf coverage cypress-coverage .nyc_output instrumented",
    "report:cy:cov": "istanbul report --include cypress-coverage/coverage.json --dir cypress-coverage lcov html text",
    "test:ci": "npm run clean:cov && npm run instrumented && start-server-andtest start http-get://localhost:3000 cy:test && npm run report:cy:cov",
    "cov:merge": "istanbul-merge --out coverage.json coverage/coveragefinal.json cypress-coverage/coverage-final.json",
    "report:cov": "istanbul report --include coverage.json --dir coverage lcov html text",
    "test:all": "npm run clean:cov && npm run test && npm run test:ci && npm run cov:merge && npm run report:cov",
    "storybook": "start-storybook -p 6006 -s public",
    "build-storybook": "build-storybook -s public"
  },
  "nyc": {
    "report-dir": "cypress-coverage",
    "exclude": [
      "instrumented/**"
    ]
  },
  "commitlint": {
    "extends": "@commitlint/config-conventional"
  },
  "semistandard": {
    "globals": [
      "describe",
      "context",
      "before",
      "beforeEach",
      "after",
      "afterEach",
      "it",
      "expect"
    ]
  },
  "jest": {
    "transform": {
      "^.+\\.[t|j]sx?$": "babel-jest"
    }
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ],
    "overrides": [
      {
        "files": [
          "**/*.stories.*"
        ],
        "rules": {
          "import/no-anonymous-default-export": "off"
        }
      }
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@babel/preset-typescript": "^7.17.12",
    "@commitlint/cli": "^17.0.1",
    "@commitlint/config-conventional": "^17.0.0",
    "@cypress/code-coverage": "^3.10.0",
    "@storybook/addon-actions": "^6.5.6",
    "@storybook/addon-essentials": "^6.5.6",
    "@storybook/addon-interactions": "^6.5.6",
    "@storybook/addon-links": "^6.5.6",
    "@storybook/builder-webpack5": "^6.5.6",
    "@storybook/manager-webpack5": "^6.5.6",
    "@storybook/node-logger": "^6.5.6",
    "@storybook/preset-create-react-app": "^4.1.1",
    "@storybook/react": "^6.5.6",
    "@storybook/testing-library": "^0.0.11",
    "@testing-library/cypress": "^8.0.2",
    "@testing-library/dom": "^8.13.0",
    "@types/jest": "^27.5.1",
    "babel-jest": "^28.1.0",
    "babel-plugin-named-exports-order": "^0.0.2",
    "babel-plugin-transform-class-properties": "^6.24.1",
    "cypress": "^9.7.0",
    "husky": "^8.0.1",
    "istanbul": "^0.4.5",
    "istanbul-merge": "^1.1.1",
    "jest": "^28.1.0",
    "nyc": "^15.1.0",
    "prop-types": "^15.8.1",
    "semistandard": "^16.0.1",
    "start-server-and-test": "^1.14.0",
    "ts-jest": "^28.0.3",
    "webpack": "^5.72.1"
  }
}
